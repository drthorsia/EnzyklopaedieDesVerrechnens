\Rightarrow\frac{2\,s}{g} & =t^{2}\qquad\biggl|\leftrightarrow\\
\Rightarrow t^{2} & =\frac{2\,s}{g}\qquad\biggl|\sqrt{\left(\right)}\\
\Rightarrow t & =\sqrt{\frac{2\,s}{g}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
